{
  "name": "BlogBuddy (BETA)",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Browser Extension for Bloggers.",
  "permissions": [
   "activeTab","contextMenus"
   ],
   "background": {
    "service_worker": "./Events/ytsearch.js"
  },
   "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons":{
    "19": "./Assets/icons/icon20.png",
    "32": "./Assets/icons/icon32.png",
    "48": "./Assets/icons/icon48.png",
    "128": "./Assets/icons/icon128.png"
  },
  "action": {
    "default_icon": "./Assets/icons/icon128.png",
    "default_title": "BlogBuddy (BETA)",
    "default_popup": "popup.html"
  }
}